{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "globals": {
    "document": true,
    "fetch": true,
    "localStorage": true,
    "window": true,
    "FormData": true
  },
  "rules": {
    "linebreak-style": 0,
    "global-require": 0,
    "no-shadow": "off",
    "eslint linebreak-style": [0, "error", "windows"],
    "max-len": ["error", 200],
    "react/require-default-props": false,
    "react/jsx-filename-extension": "off"
  }
}
